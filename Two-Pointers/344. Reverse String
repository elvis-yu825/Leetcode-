class Solution {
    public void reverseString(char[] s) {
        int low =0;
        int high = s.length-1;
        while (low < high) {
            swap(low,high, s);
            low++;
            high--;
        }
        
    }
    
    private static void swap(int low, int high, char[] s) {
        char temp = s[low];
        s[low] = s[high];
        s[high] = temp;
        
    }
}
